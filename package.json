{
  "name": "acervo-filarmonica",
  "version": "2.9.2",
  "type": "module",
  "description": "Acervo Digital da Sociedade Filarmônica 25 de Março",
  "main": "worker/index.js",
  "scripts": {
    "api": "wrangler dev",
    "api:local": "wrangler dev --local",
    "db:init": "wrangler d1 execute acervo-db --local --file=database/schema.sql && wrangler d1 execute acervo-db --local --file=database/seed-local.sql",
    "db:seed": "wrangler d1 execute acervo-db --local --file=database/seed-local.sql",
    "db:reset": "wrangler d1 execute acervo-db --local --command=\"DELETE FROM atividades; DELETE FROM favoritos; DELETE FROM partes; DELETE FROM partituras; DELETE FROM usuarios;\" && npm run db:seed",
    "dev": "wrangler pages dev frontend --d1=ACERVO_DB --r2=ACERVO_BUCKET",
    "dev:worker": "wrangler dev worker/index.js",
    "deploy": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy frontend --project-name=acervo-filarmonica",
    "setup": "node scripts/setup.js",
    "db:migrate": "wrangler d1 execute acervo-db --file=database/schema.sql",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "lint:worker": "eslint worker/src/ --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acssjr/acervo-filarmonica.git"
  },
  "keywords": [
    "filarmonica",
    "partituras",
    "musica",
    "banda"
  ],
  "author": "Antônio Júnior <acssjr@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acssjr/acervo-filarmonica/issues"
  },
  "homepage": "https://github.com/acssjr/acervo-filarmonica#readme",
  "dependencies": {
    "fflate": "^0.8.2",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.15",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "vitest": "3.2",
    "wrangler": "^4.65.0"
  }
}